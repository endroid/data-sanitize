{
    "name": "endroid/data-sanitize",
    "description": "Endroid Data Sanitize",
    "keywords": ["endroid", "data", "sanitize", "php"],
    "homepage": "https://github.com/endroid/data-sanitize",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.6.3"
    },
    "require-dev": {
        "endroid/quality": "dev-main"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\DataSanitize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\DataSanitize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
